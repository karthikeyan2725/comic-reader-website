Sql Query :

create table comic (
  id int PRIMARY KEY,
  name varchar(100) not null,
  author varchar(100),
  artist varchar(100),
  chapter_count int constraint positive_chapter_count check (chapter_count >= 0),
  language varchar(100),
  published timestamptz(0),
  description text,
  url text
)

select * from comic;
Create database dbname with template existingDbName;

