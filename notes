Promise vs Asyn/await:
 var promise = null;

    function givePromise(value){
        return  new Promise((resolve, reject)=>{
            if(value == 200) resolve("Value is 200")
            else reject("Value not 200");
        })
    }

    promise = givePromise(200)
    
    promise
        .then((message) => console.log("Success : " + message))
        .catch((message)=> console.log("fail : " + message));

    

    async function satisfy(){
        try {
            var response = await givePromise(200)
            console.log("`Success : " + response)
        } catch (err) {
            console.log("`fail : " + err)
        }
    }
    
    satisfy();

